{
  "name": "material-color-scheme",
  "version": "1.1",
  "description": "Generate a color scheme based on Material Design.",
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "rollup -c --watch",
    "build": "rollup -c",
    "lint": "eslint .",
    "docs:install": "npm install --prefix src",
    "docs:dev": "npm run dev --prefix src",
    "docs:build": "npm run build --prefix src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shockerqt/material-color-scheme.git"
  },
  "keywords": [
    "colors",
    "scheme",
    "theme",
    "material",
    "css"
  ],
  "author": "Shocker",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shockerqt/material-color-scheme/issues"
  },
  "homepage": "https://github.com/shockerqt/material-color-scheme#readme",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-typescript": "^11.1.1",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "prettier": "^2.8.8",
    "rollup": "^3.23.0",
    "tslib": "^2.5.2",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@material/material-color-utilities": "^0.2.6"
  }
}
